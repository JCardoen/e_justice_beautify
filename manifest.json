{
  "manifest_version": 2,
  "name": "e-Justice Beautify ",
  "short_name" : "e-JB",
  "description": "This extension will beautify the wall of text on the e-justice.be website to increase legibility",
  "version": "1.4",
  "browser_action": {
   "default_icon": "icon.png",
   "default_popup" : "popup.html",
   "name": "Click to beautify"
  },
  "background":{
            "scripts":["background.js"],
            "persisten":false
        },
  
  "permissions": [
   "activeTab"
   ]
}